@page "/users"

<AuthorizeView Roles="admin">
    <Authorized>
        @if (task.IsCompleted)
        {
            <GridComponent T="Auth0.ManagementApi.Models.User" Grid="@grid"></GridComponent>
        }
        else
        {
            <SpinLoader IsLoading="@(true)" Spinner="SpinnerType.Circle"/>
        }
    </Authorized>
    <NotAuthorized>
        You have to login
    </NotAuthorized>
</AuthorizeView>

